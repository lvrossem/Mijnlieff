<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="201.0" prefWidth="439.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mijnlieff.controllers.NameSelectController">
   <children>
      <TextField fx:id="nameField" layoutX="129.0" layoutY="101.0" />
      <Label layoutX="129.0" layoutY="82.0" text="Kies een gebruikersnaam:" />
      <Button fx:id="nameConfirm" layoutX="282.0" layoutY="98.0" mnemonicParsing="false" onAction="#processName" text="OK" />
      <Label fx:id="errorLabel" layoutX="219.0" layoutY="165.0" prefHeight="18.0" prefWidth="6.0" textFill="#e80606" />
      <Button layoutX="14.0" layoutY="158.0" mnemonicParsing="false" onAction="#quit" text="Spel verlaten" />
   </children>
</AnchorPane>
